{
  "name": "prospectos_client",
  "version": "1.0.0",
  "description": "CRUD Básico para el manejo y administración de prospectos a clientes por parte de los promotores",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-shared": "ng serve --host 0.0.0.0 --port 2100",
    "build": "ng build --configuration production",
    "build-osx": "ng build --configuration production",
    "test": "jest --detectOpenHandles",
    "test-watch": "jest --watchAll",
    "test-coverage": "jest --coverage",
    "unit-test": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s",
    "json-start": "nodemon server.js",
    "json-start-shared": "nodemon server.js --host 0.0.0.0 --port 3200",
    "dev": "npm-run-all --parallel start json-start",
    "dev:hmr": "npm-run-all --parallel hmr json-start",
    "dev:shared": "npm-run-all --parallel start-shared json-start-shared"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Diegoztag/prospectos_client.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  },
  "keywords": [],
  "author": "Diego Zazueta",
  "license": "ISC",
  "private": true,
